gStar = [];\n\n    for (let i = 1; i <= 5; i += 1) {\n      if (i <= ratedStar) {\n        ratingStar.push(<Star key={i} />);\n      } else {\n        ratingStar.push(<StarOff key={i} />);\n      }\n    }\n    return ratingStar.map(star => star);\n  };\n\n  const onEditModal = () => {\n    setEditModal(true);\n  };\n\n  const offEditModal = () => {\n    setEditModal(false);\n  };\n\n  const deleteReview = () => {\n    if (!auth) {\n      Swal.fire({\n        icon: 'error',\n        text: 'Î°úÍ∑∏Ïù∏ÏùÑ Î®ºÏ†Ä Ìï¥Ï£ºÏÑ∏Ïöî‚ùóÔ∏è',\n        confirmButtonColor: '#FFD337',\n      }).then(res => {\n        navi('/login');\n      });\n    } else {\n      Swal.fire({\n        // title: 'ÎåìÍ∏ÄÏùÑ ÏÇ≠Ï†úÌïòÏãúÍ≤†ÏäµÎãàÍπå?',\n        text: 'ÌõÑÍ∏∞Î•º ÏÇ≠Ï†úÌïòÏãúÍ≤†ÏäµÎãàÍπå?',\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#FFD337',\n        cancelButtonColor: '#ffffff',\n        confirmButtonText: 'ÎÑ§',\n        cancelButtonText: '<span style=\"color:#000000\">ÏïÑÎãàÏò§<span>',\n      }).then(result => {\n        axios\n          .delete(`${apiUrl}/api/review/${kinderInfo.reviewId}`, {\n            headers: {\n              Authorization: localStorage.getItem('token'),\n            },\n          })\n          .then(response => {\n            Swal.fire({\n              confirmButtonColor: '#FFD337',\n              text: 'ÌõÑÍ∏∞Í∞Ä ÏÇ≠Ï†úÎêòÏóàÏäµÎãàÎã§.',\n            }).then(result => {\n              window.location.reload();\n            });\n          })\n          .catch(error => {\n            if (error.response && error.response.status === 403) {\n              Swal.fire({\n                icon: 'error',\n                text: 'Î≥∏Ïù∏Ïù¥ ÏûëÏÑ±Ìïú ÌõÑÍ∏∞Îßå ÏÇ≠Ï†úÌï† Ïàò ÏûàÏñ¥Ïöî‚ùóÔ∏è',\n                confirmButtonColor: '#FFD337',\n              });\n            } else if (error.response && error.response.status === 401) {\n              Swal.fire({\n                icon: 'error',\n                text: 'ÌÜ†ÌÅ∞Ïù¥ ÎßåÎ£